<div class="modal fade" id="staticBackdrop_<%= application.id  %>" data-backdrop="static" data-keyboard="false" tabindex="-1" aria-labelledby="staticBackdropLabel" aria-hidden="true">
  <div class="modal-dialog modal-dialog-scrollable">
    <div class="modal-content">
      <div class="modal-body">
        <div>
          <% if application.file.attached? %>
          <%= cl_image_tag(application.file.key, :width => 200, :height => 250, :crop => :fill, :page => 1) %>
          <% else %>
           <p> El solicitante no subi√≥ ningun archivo</p>
          <% end %>
        </div>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
      </div>
    </div>
  </div>
</div>

